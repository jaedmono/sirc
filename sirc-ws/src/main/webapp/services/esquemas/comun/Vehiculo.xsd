<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns="http://www.movilidadbogota.gov.co/esb/schema/Vehiculo/v1"
	elementFormDefault="unqualified"
	targetNamespace="http://www.movilidadbogota.gov.co/esb/schema/Vehiculo/v1"
	version="1.0">

	<!--Entidad que hace referencia al vehiculo del conductor-->
	<xsd:complexType name="Vehiculo">
		<xsd:sequence>
			<!-- Placa del vehiculo -->
			<xsd:element maxOccurs="1" minOccurs="1" name="placa" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
